<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
      <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
        <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="ProductPremiumCollectorService.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
        </sectionGroup>
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/>
    </startup>
  <log4net>
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="ProductPremiumCollectorService.log"/>
      <appendToFile value="true"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="10"/>
      <maximumFileSize value="10MB"/>
      <staticLogFileName value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %level %logger - %message%newline"/>
      </layout>
    </appender>
    <logger name="ErrorFile">
      <appender-ref ref="RollingFileAppender"/>
    </logger>
    <root>
      <level value="DEBUG"/>
      <appender-ref ref="RollingFileAppender"/>
    </root>
  </log4net>
    <applicationSettings>
        <ProductPremiumCollectorService.Properties.Settings>
            <setting name="DbConnectionString" serializeAs="String">
                <value>data source=tst-sql-db-2;initial catalog=MobileRaterService;integrated security=True;</value>
            </setting>
            <setting name="WIS_BaseURL" serializeAs="String">
                <value>https://lpamw-test.woodmen.net:2015/</value>
            </setting>
            <setting name="MobileRaterService_BaseURL" serializeAs="String">
                <value>https://lpamw-test.woodmen.net/mobileraterservice/</value>
            </setting>
            <setting name="SMTPServer" serializeAs="String">
                <value>wesg.woodmen.net</value>
            </setting>
            <setting name="SMTPFromAddress" serializeAs="String">
                <value>autoMessage@woodmen.org</value>
            </setting>
            <setting name="EmailList" serializeAs="Xml">
                <value>
                    <ArrayOfString xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <string>unitdevs@woodmen.org</string>
                    </ArrayOfString>
                </value>
            </setting>
            <setting name="TransactionPath" serializeAs="String">
                <!--<value>C:\Program Files (x86)\WoodmenLife\Product Premium Collector Service\</value>-->
              <value>d:\programfiles\WoodmenLife\Product Premium Collector Service\</value>
            </setting>
        </ProductPremiumCollectorService.Properties.Settings>
    </applicationSettings>
</configuration>
