<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
        <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="LpaDataFix.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
        </sectionGroup>
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/>
    </startup>
    <applicationSettings>
        <LpaDataFix.Properties.Settings>
            <setting name="LpaServiceName" serializeAs="String">
                <value>LifePortraitsWebServer - WOW</value>
            </setting>
            <setting name="LpaSyncServiceName" serializeAs="String">
                <value>LifeManagerService - WOW</value>
            </setting>
            <setting name="SqlQuery" serializeAs="String">
                <value>DECLARE @state int; DECLARE @mode int; select top 1 @state = ApplicationState from [dbo].[Scenario] WHERE ApplicationState IS NOT NULL GROUP BY ApplicationState ORDER BY COUNT(ApplicationState) DESC; select top 1 @mode = PremiumMode from [dbo].[Scenario] WHERE PremiumMode IS NOT NULL GROUP BY PremiumMode ORDER BY COUNT(PremiumMode) DESC; insert into dbo.Scenario select [ScenarioID], null, null, [ProspectID], @state, null, @mode, null from [dbo].[ScenePointer] where [ScenarioID] not in (select [ScenarioID] from [dbo].[Scenario]);</value>
            </setting>
            <setting name="ObjRefSql" serializeAs="String">
                <value>DECLARE @NullScenario TABLE (SceneID UNIQUEIDENTIFIER, NewGuid UNIQUEIDENTIFIER);INSERT INTO @NullScenario SELECT [SceneID], NEWID() as NewGuid FROM [ScenePointer] where typecode = 32 and ScenarioID is null; Insert into [Scenario] ([ScenarioID],[OwnerType],[OrgOwnerID],[PersonOwnerID],[ApplicationState],[EffectiveDate],[PremiumMode],[SignedDate]) Select NewGuid, 1, null, null, 17, null, 1, null from @NullScenario; Update sp Set sp.ScenarioID = ns.NewGuid from [ScenePointer] sp inner join @NullScenario ns on sp.SceneID = ns.SceneID;</value>
            </setting>
            <setting name="LpaConnectionString" serializeAs="String">
                <value>Data Source=(LocalDB)\v11.0;Initial Catalog=WOWLPES;AttachDBFilename=C:\\LPLADesktop\\WOW\\Databases\\WOWLPES.mdf;Integrated Security=True</value>
            </setting>
        </LpaDataFix.Properties.Settings>
    </applicationSettings>
